Package: tsduck-dev
Source: tsduck
Version: {{VERSION}}
Maintainer: Thierry Lelegard <thierry@lelegard.fr>
Architecture: {{ARCH}}
Section: free/video
Priority: optional
Depends: tsduck,
 libsrt1,
 libsrt-dev,
 libpcsclite1,
 libpcsclite-dev,
 libcurl3 | libcurl4,
 libcurl3-dev | libcurl4-dev | libcurl4-openssl-dev | libcurl4-nss-dev | libcurl4-gnutls-dev,
